, 			{
				"box" : 				{
					"code" : "History ephase, ophase, smoothed;\r\n\r\nBuffer grain_buffer(\"grain_buffer\");\ngrain_id = in1; \nhealth, p0, p1, p2, x, y, vx, vy = peek(grain_buffer, grain_id, 0, channels=8);\n\r\nspawn = abs(noise()) < 0.001;\r\ntrigger = spawn && health > 0;\r\nstart = in2+1+abs(noise())*schedahead;\r\n\r\nbeetle_dur = 3/8 ; \r\nbeetle_frequency = 600;\r\nbeetle_filter = 0.9;\r\nbeetle_rpts = 1;\r\nbeetle_modulation = 2.2*abs(noise());;\r\n\r\nl0, l1, r0, r1 = 0, 0, 0, 0;\r\n\r\nif (trigger) {\r\n\tisr = 1/samplerate;\r\n\t\r\n\t/*\r\n\tp0 = abs(noise());\r\n\tp1 = abs(noise());\r\n\tp2 = abs(noise());\r\n\t*/\r\n\t\r\n\tamp = 0.5*abs(noise());\r\n\t\r\n\tperiod = .5*(1+p0); //beetle.period\r\n\tdur = period * beetle_dur; //period * beetle_dur * beetle.scale.z;\r\n\tlen = dur * samplerate;\r\n\t\r\n\tfreq = beetle_frequency/period; \r\n\toinc = freq * isr;\r\n\tophase = abs(noise()); //0.; \r\n\tenvinc = ((beetle_rpts + floor(p2 * beetle_rpts)) / dur) * isr;\n\tephase = 0.;\n\tfilter = beetle_filter; //clamp(beetle.energy * beetle_filter, 0., 1.);\r\n\tfilter2 = 1.-(filter*filter);\n\tsmoothed = 0.;\n\tomod = (p1+0.5)*beetle_modulation; //beetle.energy * beetle_modulation;\r\n\t\r\n\t// TODO set up pan\r\n\tl0 = x*y;\r\n\tr0 = (1-x)*y;\r\n\tl1 = x*(1-y);\r\n\tr1 = (1-x)*(1-y);\r\n\t\r\n\tfor (i=0; i<len; i+= 1) {\r\n\t\tephase = ephase + envinc; //i/(len-1);\r\n\t\tw0 = sample(win, ephase, boundmode=\"wrap\");\r\n\t\t\r\n\t\tophase += oinc * (1. + -0.001*omod*ophase);\r\n\t\tosc = sample(rep3, ophase, boundmode=\"wrap\");\r\n\t\t\r\n\t\toe = amp*osc*w0*w0;\r\n\t\ts = oe + filter2*(smoothed - oe);\r\n\t\tsmoothed = s;\r\n\t\t\r\n\t\t// pan s\r\n\t\tpoke(ola3, s*l0, start+i, 0, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola3, s*r0, start+i, 1, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola3, s*l1, start+i, 2, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola3, s*r1, start+i, 3, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t}\r\n\r\n\t// schedule next grain to follow:\n\t//beetle.at = (-(period * samplerate));\r\n\t\r\n}",
