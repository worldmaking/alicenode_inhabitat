				"box" : 				{
					"code" : "filter(input, a0, a1, a2, b1, b2) {\n\tHistory x2(0);\n\tHistory y2(0);\n\tHistory x1(0);\n\tHistory y1(0);\n\tmul_1 = x2 * a2;\n\tmul_2 = x1 * a1;\n\tmul_3 = input * a0;\n\tmul_4 = y1 * b1;\n\tmul_5 = y2 * b2;\n\tsub_6 = ((((mul_3 + mul_2)) + mul_1)) - ((mul_5 + mul_4));\n\tresult = sub_6;\n\tx2_next_7 = fixdenorm(x1);\n\ty2_next_8 = fixdenorm(y1);\n\tx1_next_9 = fixdenorm(input);\n\ty1_next_10 = fixdenorm(sub_6);\n\tx2 = x2_next_7;\n\ty1 = y1_next_10;\n\tx1 = x1_next_9;\n\ty2 = y2_next_8;\r\n\treturn result;\r\n}\r\n\r\nHistory ephase, ophase, smoothed;\r\n\r\nBuffer grain_buffer(\"grain_buffer\");\ngrain_id = in1;\n//health, p0, p1, p2, x, y, vx, vy = peek(grain_buffer, grain_id, 0, channels=8);\r\nstate, speaker, health, age, size, p0, p1, p2 = peek(grain_buffer, grain_id, 0, channels=8);\r\n\r\nspawn = abs(noise()) < 0.0006;\r\ntrigger = spawn && absdiff(state, 0.2) < 0.01;\r\nstart = in2+1+abs(noise())*schedahead;\r\n\r\nbeetle_dur = 1/9 ; \r\nbeetle_frequency = 500 + size ;\r\nbeetle_filter = 1; // DON'T SET A PARAM TO THIS, UNLESS YOU RESTRICT IT TO WITHIN 0.-1\r\nbeetle_rpts = 3;\r\nbeetle_modulation = 10 * abs(health);\r\n\r\nl0, l1, r0, r1 = 0, 0, 0, 0;\r\n\r\nif (trigger) {\r\n\tisr = 1/samplerate;\r\n\t\r\n\t/*\r\n\tp0 = abs(noise());\r\n\tp1 = abs(noise());\r\n\tp2 = abs(noise());\r\n\t*/\r\n\t\r\n\tamp = health + 1;\r\n\t\r\n\tperiod = .12*(1+p0); //beetle.period\r\n\tdur = period * beetle_dur; //period * beetle_dur * beetle.scale.z;\r\n\tlen = dur * samplerate;\r\n\t\r\n\tfreq = beetle_frequency/period; \r\n\toinc = freq * isr;\r\n\tophase = abs(noise()); //0.; \r\n\tenvinc = ((beetle_rpts + floor(age * beetle_rpts)) / dur) * isr;\n\tephase = 0.;\n\tfilter = beetle_filter; //clamp(beetle.energy * beetle_filter, 0., 1.);\r\n\tfilter2 = 1.-(filter*filter);\n\tsmoothed = 0.;\n\tomod = (health+0.5)*beetle_modulation; //beetle.energy * beetle_modulation;\r\n\t\r\n\t// TODO set up pan & add 5th channel!\r\n\tch0 = absdiff(speaker, 0.0) < 0.01;\r\n\tch1 = absdiff(speaker, 0.1) < 0.01;\r\n\tch2 = absdiff(speaker, 0.2) < 0.01;\r\n\tch3 = absdiff(speaker, 0.3) < 0.01;\r\n\tch4 = absdiff(speaker, 0.4) < 0.01;\r\n\t\r\n\t\r\n\tfor (i=0; i<len; i+= 1) {\r\n\t\tephase = ephase + envinc; //i/(len-1);\r\n\t\tw0 = sample(win2, ephase, boundmode=\"wrap\");\r\n\t\t\r\n\t\tophase += oinc * (1. + -0.001*omod*ophase);\r\n\t\tosc = sample(rep2, ophase, boundmode=\"wrap\");\r\n\t\t\r\n\t\toe = amp*osc*w0*w0;\r\n\t\ts = oe + filter2*(smoothed - oe);\r\n\t\tsmoothed = s;\r\n\t\t\r\n\t\ts = filter(s, a0, a1, a2, b1, b2);\r\n\t\t\r\n\t\t// pan s\r\n\t\tpoke(ola2, s*ch0, start+i, 0, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola2, s*ch1, start+i, 1, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola2, s*ch2, start+i, 2, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola2, s*ch3, start+i, 3, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t\tpoke(ola2, s*ch4, start+i, 4, 1, overdubmode=\"accum\", boundmode=\"wrap\");\r\n\t}\r\n\r\n\t// schedule next grain to follow:\n\t//beetle.at = (-(period * samplerate));\r\n\t\r\n}",
